    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Module",
            "type": "debugpy",
            "request": "launch",
            "module": "solar",
            "args": "${command:pickArgs}"
        },
        {
            "name": "Python Debugger: solar",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/solar/solar.py",
            "console": "integratedTerminal",
            "envFile": "${workspaceFolder}/.env"
        }
    ]
}